            if (!Rs&noDisplay) {
                $d = me&Display,R,{prep:1}
                
                # < appears twice unless:
                Rs&noDisplay = 1
            }
            me&applyRbitX,R
        },'ift,D'],
        
       // %Pier,C,E,D types @2, &Eing E/** @3
        ['Pier',0.2,&acgtRs{
